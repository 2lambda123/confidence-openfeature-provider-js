{
  "name": "@spotify-confidence/integration-next",
  "license": "Apache-2.0",
  "version": "0.0.0",
  "module": "build/esm/index.js",
  "main": "build/cjs/index.js",
  "types": "build/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./build/cjs/index.js",
      "import": "./build/esm/index.js",
      "types": "./build/types/index.d.ts"
    },
    "./Next13": {
      "require": "./build/cjs/OpenFeatureNext13.js",
      "import": "./build/esm/OpenFeatureNext13.js",
      "types": "./build/types/OpenFeatureNext13.d.ts"
    },
    "./Next12": {
      "require": "./build/cjs/OpenFeatureNext12.js",
      "import": "./build/esm/OpenFeatureNext12.js",
      "types": "./build/types/OpenFeatureNext12.d.ts"
    }
  },
  "peerDependencies": {
    "@openfeature/web-sdk": "^0.3.10-experimental",
    "react-dom": ">=16 <19"
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rm -rf build"
  },
  "devDependencies": {
    "@spotify-confidence/integration-react": "^0.0.1",
    "@spotify-confidence/openfeature-web-provider": "^0.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node-fetch": "^2.6.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
